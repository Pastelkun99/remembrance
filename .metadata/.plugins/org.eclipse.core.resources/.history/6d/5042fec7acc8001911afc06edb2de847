<mapper namespace="wordMapper">
	<select id="selectWordList" parameterType="java.lang.String" resultType="com.project.dao.Word">
		SELECT Q_NO, Q_ID, Q_FRONT, Q_BACK, Q_HIT FROM ${a} ORDER BY Q_NO ASC
	</select>
